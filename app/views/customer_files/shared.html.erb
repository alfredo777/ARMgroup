<h1>Archivos compartidos con el usuario</h1>
<div class="row">
  <div class="col-lg-12">
    <div class="col-md-9">
      <table class="table" id="files">
        <tr>
          <th>Archivo</th>
          <th>Opcciones</th>
        </tr>
        <% @customer.shared_files.each do |f| %>
        <%= render partial: 'admin/file', locals: {f: f, permited: false} %>
        <%end%>
      </table>
    </div>
  </div>
</div>
